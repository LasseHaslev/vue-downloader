{
  "name": "@lassehaslev/vue-downloader",
  "description": "Image picker using @lassehaslev/vue-item-picker",
  "version": "0.0.1",
  "author": "Lasse S. Haslev <lasse@haslev.no>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lassehaslev/vue-downloader.git"
  },
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "dev": "npm-run-all --parallel serve data-server",
    "build": "rm -rf dist/ && mkdir dist && babel src --out-dir dist --ignore tutorials/,main.js",
    "prepublish": "npm run build",
    "data-server": "nodemon  src/tutorials/server/server.js"
  },
  "dependencies": {
    "@lassehaslev/vue-modal": "^3.1.1",
    "axios": "^0.15.3",
    "downloadjs": "^1.4.7",
    "vue": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "multer": "^1.2.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  }
}
